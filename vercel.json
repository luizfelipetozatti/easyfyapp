{
  "buildCommand": "pnpm --filter @agendazap/database db:generate && pnpm --filter @agendazap/web build",
  "outputDirectory": "apps/web/.next",
  "installCommand": "pnpm install --frozen-lockfile",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 10
    },
    "app/dashboard/**/*.tsx": {  
      "maxDuration": 10
    }
  },
  "env": {
    "PRISMA_GENERATE_DATAPROXY": "false",
    "SKIP_ENV_VALIDATION": "true"
  },
  "crons": []
}